{
  "name": "cypress-end-to-end",
  "version": "1.0.0",
  "description": "Sample project to demonstrate e2e tests written with Cypress running on GitHub Actions.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint cypress/**/*.js",
    "fix": "eslint cypress/**/*.js --fix",
    "cy:open": "cypress open",
    "cy:open:tablet": "cypress open --config viewportWidth=767,viewportHeight=480",
    "test": "cypress run",
    "test:tablet": "cypress run --config viewportWidth=767,viewportHeight=480 --spec cypress/integration/authenticatedScenarios.spec.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^8.2.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-iframe": "^1.0.1",
    "cypress-mailosaur": "^2.3.3",
    "eslint": "^7.32.0",
    "eslint-plugin-cypress": "^2.11.3",
    "faker": "^5.5.3"
  }
}
